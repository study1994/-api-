<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.dao.IUserInfoDao">
    <select id="queryUserInfoByUid" parameterType="int" resultType="com.test.bean.UserInfo">
        select * from tab_user_info where uid = #{uid}
    </select>
</mapper>
